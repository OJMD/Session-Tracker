					<Color r="0.5" g="0.7" b="0.9" a="1" />
					<FontHeight val="12" />
					<Anchors>
						<Anchor point="TOPRIGHT" relativeTo="$parent" x="-15" y="-50" />
					</Anchors>
					<Shadow>
						<Offset x="1" y="1" />
						<Color r="0" g="0" b="0" />
					</Shadow>
				</FontString>
			</Layer>
